{
	"name": "BigTable",
	"version": "1.5.0",
	"author": [
		"wiki.gg development team",
		"[https://github.com/MrPie5 Mr Pie 5]",
		"[https://github.com/alex4401 alex4401]"
	],
	"url": "https://wiki.gg",
	"descriptionmsg": "extbigtable-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.43.0",
		"platform": {
			"php": ">= 8.1"
		}
	},
	"MessagesDirs": {
		"BigTable": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "BigTable/modules"
	},
	"ResourceModules": {
		"ext.bigtable": {
			"dependencies": [
				"jquery",
				"mediawiki.util"
			],
			"packageFiles": [
				"ext.bigtable/main.js",
				"ext.bigtable/features/columnHover.js",
				"ext.bigtable/features/scrollOverlay.js",
				"ext.bigtable/features/stickyHeader.js"
			]
		},
		"ext.bigtable.styles": {
			"styles": [
				"ext.bigtable.styles/main.less"
			]
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\BigTable\\": "src/"
	},
	"OutputPipelineStages": [
		{
			"class": "MediaWiki\\Extension\\BigTable\\OutputTransform\\TableTransformPipelineStage"
		}
	],
	"config_prefix": "wgBigTable",
	"config": {},
	"manifest_version": 2
}
